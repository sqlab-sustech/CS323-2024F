        float e=add(a,c);
    }
    while(d>a){
        if(e>c || a==b){
            if(c>e || (a>10)){
               a=0;
               a=b*c;
               b=c-a;
               a=b/d;
            }else{
                 if(e>=c && !(c>a)){
